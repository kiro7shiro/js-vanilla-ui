<div class="w3-bar w3-top w3-black w3-large" style="z-index:4" id="myNavbar2">
    <button class="w3-bar-item w3-button w3-hide-large w3-hover-none w3-hover-text-light-grey"
        data-action="sidebar-open"><i data-action="sidebar-open" class="fa fa-bars"></i> &nbsp;Menu</button>
    <% items.forEach(function (item) { %>
        <button class="w3-bar-item w3-button w3-hover-none w3-hover-text-light-grey" data-action="<%= item %>-click"><i
                data-action="<%= item %>-click" class="fa fa-bars"></i> &nbsp;<%= item %></button>
    <% }); %>
    <span class="w3-bar-item w3-right">Logo</span>
</div>